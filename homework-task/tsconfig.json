{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "@homework-task/*": ["*"]
      },
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": true
    },
    "include": ["src"]
  }
  